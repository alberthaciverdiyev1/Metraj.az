    const template = document.getElementById("brand-template");
    const track = document.getElementById("carousel-track");
    const trackClone = document.getElementById("carousel-track-clone");

    logos.forEach(src => {
        const original = template.content.cloneNode(true);
        original.querySelector("img").src = src;
